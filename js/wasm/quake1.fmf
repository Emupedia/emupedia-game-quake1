game Quake
name "Quake 1"
basegame id1

bind w +forward
bind s +back
bind a +moveleft
bind d +moveright
bind mouse2 +jump
bind mouse3 +forward
bind mwheelup impulse 10
bind mwheelup impulse 12

package id1/pak0.pak - "../../assets/id1/pak0.zip"
package id1/pak1.pak - "../../assets/id1/pak1.zip"
package id1/pak2.pak - "../../assets/id1/pak2.zip"